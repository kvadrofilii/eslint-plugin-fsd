{
    "name": "@michael-yakovlev/eslint-plugin-fsd",
    "version": "0.2.0",
    "type": "module",
    "license": "MIT",
    "description": "FSD eslint rules",
    "keywords": [
        "eslint",
        "eslintplugin",
        "eslint-plugin",
        "clean code",
        "code cleaner",
        "FSD",
        "Feature-Sliced Design"
    ],
    "author": "Michael Yakovlev",
    "files": [
        "dist"
    ],
    "main": "./dist/index.mjs",
    "module": "./dist/index.mjs",
    "types": "./dist/index.d.mts",
    "exports": {
        ".": "./dist/index.mjs",
        "./package.json": "./package.json"
    },
    "scripts": {
        "build": "tsdown --sourcemap --minify",
        "dev": "tsdown --watch",
        "type-check": "tsc --noEmit",
        "lint": "eslint",
        "format": "prettier *.ts --write",
        "test": "vitest",
        "prepublishOnly": "pnpm run build"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/kvadrofilii/eslint-plugin-fsd"
    },
    "prettier": "@michael-yakovlev/prettier-config",
    "publishConfig": {
        "access": "public"
    },
    "packageManager": "pnpm@10.23.0",
    "devDependencies": {
        "@michael-yakovlev/eslint-config": "^0.1.0",
        "@michael-yakovlev/prettier-config": "^1.0.7",
        "@types/node": "^24.10.1",
        "eslint": "^9.39.1",
        "eslint-vitest-rule-tester": "^3.0.0",
        "jiti": "^2.6.1",
        "prettier": "^3.6.2",
        "tsdown": "^0.16.6",
        "typescript": "^5.9.3",
        "vitest": "^4.0.13"
    },
    "engines": {
        "node": "^18.18.0 || ^20.9.0 || >= 21.1.0"
    },
    "peerDependencies": {
        "eslint": ">=9.39.1"
    }
}
